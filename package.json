{
  "name": "wivex",
  "version": "0.1.4",
  "description": "wivex is a JavaScript library that allows dynamic loading of HTML components and makes it easy to create routes in web applications. With wivex, you can load HTML components asynchronously, manage routes in a simple way, and inject custom elements into the head of your document.",
  "type": "module",
  "main": "dist/wivex.cjs.js",
  "module": "dist/wivex.es.js",
  "bin": {
    "wivex": "./bin/Cli.js"
  },
  "files": [
    "bin/",
    "commands/",
    "core/",
    "dist/",
    "plugins/",
    "prebuild/",
    "template/",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/wivex.es.js",
      "require": "./dist/wivex.cjs.js"
    },
    "./core": "./core/compiler.js",
    "./plugin": "./plugins/vite-plugin-wivex-compiler.js",
    "./prebuild": "./prebuild/index.js"
  },
  "scripts": {
    "dev": "vite sandbox --host",
    "tests": "witests",
    "build": "vite build"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "commander": "^12.1.0",
    "gh-pages": "^6.2.0",
    "magic-string": "^0.30.14",
    "vite": "^5.4.10"
  },
  "keywords": [
    "wivex",
    "wipodev",
    "cli",
    "web",
    "component",
    "dynamic",
    "loading",
    "html",
    "javascript",
    "library",
    "routes",
    "template"
  ],
  "author": "wipodev",
  "license": "MIT",
  "devDependencies": {
    "witests": "^0.0.1"
  }
}
