{
  "name": "wiview",
  "version": "1.0.1",
  "description": "wiview is a JavaScript library that allows dynamic loading of HTML components and makes it easy to create routes in web applications. With wiview, you can load HTML components asynchronously, manage routes in a simple way, and inject custom elements into the head of your document.",
  "type": "module",
  "main": "dist/wiview.cjs.js",
  "module": "dist/wiview.es.js",
  "bin": {
    "wiview": "./bin/Cli.js"
  },
  "files": [
    "bin/",
    "commands/",
    "dist/",
    "plugins/",
    "prebuild/",
    "template/",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/wiview.es.js",
      "require": "./dist/wiview.cjs.js"
    },
    "./plugin": "./plugins/vite-plugin-wiview.js",
    "./prebuild": "./prebuild/index.js"
  },
  "scripts": {
    "dev": "vite sandbox --host",
    "build": "vite build"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "commander": "^12.1.0",
    "gh-pages": "^6.2.0",
    "jsdom": "^25.0.1",
    "vite": "^5.4.10"
  },
  "keywords": [
    "wiview",
    "wipodev",
    "cli",
    "web",
    "component",
    "dynamic",
    "loading",
    "html",
    "javascript",
    "library",
    "routes",
    "template"
  ],
  "author": "wipodev",
  "license": "MIT"
}
